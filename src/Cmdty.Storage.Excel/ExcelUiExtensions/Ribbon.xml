<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns='http://schemas.microsoft.com/office/2009/07/customui'>
	<ribbon>
		<tabs>
			<tab id='cmdtyStorageTab' label='Cmdty.Storage' keytip='S'>
				<group id='calculationGroup' label='Calculate'>
					<button id='calcPendingButton' label='Calculate All Pending' size='large' onAction='CalculateAllPending' imageMso='Calculator' keytip='C' 
					        supertip='Ctrl+Shift+A'/>
					<button id='cancelAll' label='Cancel All Running' size='large' onAction='CancelExecution' imageMso='X' supertip='Ctrl+Shift+X'/>
				</group>
			</tab>
		</tabs>
	</ribbon>
	<contextMenus>
		<contextMenu idMso='ContextMenuCell'>
			<menu id='CmdtyStorageMenu' label='Cmdty.Storage' insertBeforeMso='Cut' >
				<button id='calculateSelectedMenuButton' label='Calculate Selected   Ctrl+Shift+S' imageMso='Calculator' onAction='CalculateSelected'/>
				<button id='cancelSelectedMenuButton' label='Cancel Selected       Ctrl+Shift+C' imageMso='X' onAction='CancelSelected'/>
			</menu>
			<menuSeparator id='MySeparator' insertBeforeMso='Cut' />
		</contextMenu>
	</contextMenus>
</customUI>
